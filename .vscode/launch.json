    {
        "name": "MBSE Python attach in docker",
        "type": "python",
        "request": "attach",
        "connect": {
            "host": "localhost",
            "port": 5678
        },
        "cwd": "${workspaceFolder}/scripts",
        "console": "integratedTerminal",
        "justMyCode": false,
        "pathMappings": [
            {
                "localRoot": "/home/reggiemarr/Projects/MBSE_FSW/",
                "remoteRoot": "/MBSE_FSW"
            },
            {
                "localRoot": "/home/reggiemarr/Projects/MBSE_FSW/scripts/",
                "remoteRoot": "/MBSE_FSW/scripts"
            }
        ]
    },
    {
        "name": "pyinfra deploy",
        "type": "python",
        "request": "launch",
        "module": "pyinfra",
        "env": {
            "GEVENT_SUPPORT": "True"
        },
        "args": [
            "--limit",
            "obc-c441",
            "inventory.py",
            "inventory.py",
            "deploy.py"
        ],
        "cwd": "${workspaceFolder}/deploy",
        "console": "integratedTerminal",
        "justMyCode": false
    },
    {"name": "run deployment",
        "type": "python",
        "request": "launch",
        "args": [
            "${workspaceFolder}/run-paramiko.py"
        ],
        "console": "integratedTerminal",
        "justMyCode": false
    },
