    "sourceFileMap": {
        "/fsw/": "${workspaceFolder}"
    },
    "setupCommands": [
        {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
        }
    ],
    "pipeCwd": "${workspaceFolder}",
    "pipeProgram": "docker",
    "pipeArgs": [
        "compose",
        "run",
        "-i",
        "fsw",
        "bash",
        "-c"
    ],
    "debuggerPath": "/usr/bin/gdb"
},

